<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.2.xsd">
	<bean class="com.iconnect.profiling.service.RegistrationServiceImpl">
		<constructor-arg name="domain" ref="regDomain" />
		<constructor-arg name="repository" ref="daoImpl" />
		<property name="companyInfo" ref="comapanyInfo" />
	</bean>
	<bean id="service" class="com.iconnect.profiling.service.LoginServiceImpl">
		<constructor-arg name="loginRepository" ref="loginRepository" />
	</bean>
	
	<bean id="regDomain" class="com.iconnect.profiling.domain.RegistrationDomain" />
	<bean id="comapanyInfo" class="com.iconnect.profiling.domain.CompanyInfoDomain" />

</beans>
